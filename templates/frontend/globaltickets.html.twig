{% extends "_layout.html.twig" %}


{% block topbar %}
    {{ include('/backend/user_topbar.html') }}
{% endblock %}

{% block body %}
    <div class="card m-1">
        <div class="card-header">
            <h6>GLOBAL TICKETS</h6>
        </div>
        <div class="card-body">
            {% for ticket in tickets %}
                - {{ ticket.title }}
            {% endfor %}
        </div>
    </div>
{% endblock %}